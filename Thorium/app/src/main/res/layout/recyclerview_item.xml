        android:id="@+id/jitter"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="@string/jitter"
        android:textSize="16sp" />

</LinearLayout>